    "import numpy as np\n",
    "from scipy.stats import multinomial\n",
    "import matplotlib.pyplot as plt\n",
    "N, t = 10, [0.3, 0.2, 0.5]\n",
    "S = multinomial(N, t)\n",
    "print(S.pmf([3, 2, 5]))\n",
    "print(S.rvs(4))\n",
    "plt.bar(np.arange(1,len(t)+1), S.p)\n",
    "plt.show()"
   ]
